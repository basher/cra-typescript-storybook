{
  "name": "cra-typescript-storybook",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-scripts": "3.1.1",
    "typescript": "3.5.3",
    "what-input": "^5.2.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.0-beta.38",
    "@storybook/addon-docs": "^5.2.0-beta.38",
    "@storybook/addon-links": "^5.2.0-beta.38",
    "@storybook/addons": "^5.2.0-beta.38",
    "@storybook/react": "^5.2.0-beta.38",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.16",
    "postcss-cli": "^6.1.2",
    "postcss-scss": "^2.0.0",
    "postcss-sorting": "^5.0.0",
    "prettier": "^1.17.1",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-order": "^0.8.1",
    "stylelint-scss": "^3.6.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "ie 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.scss": [
      "postcss -c postcss.config.js  --no-map -r",
      "prettier --single-quote --write",
      "git add"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
